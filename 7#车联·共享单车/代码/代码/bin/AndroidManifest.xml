<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hanheng.dialswitch1"
    android:versionCode="1"
    android:versionName="1.0" >
	
    <uses-permission android:name="android.permission.WAKE_LOCK" />
	<uses-permission android:name="android.Manifest.permission.DEVICE_POWER"/>
	
    <uses-permission android:name="android.permission.READ_SETTINGS" />
	<uses-permission android:name="android.permission.WRITE_SETTINGS" />
	
    <uses-permission android:name="android.permission.CHANGE_CONFIGURATION"/>
    
    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="17" />
    

    <application
        android:allowBackup="true"
        android:icon="@drawable/shen"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" android:hasCode="true">
        <activity
            android:name="com.hanheng.dialswitch.MainActivity"
            android:label="@string/app_name" 
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.hanheng.dialswitch.secondactivity" >
		</activity>
        <activity android:name="com.hanheng.dialswitch.thirdActivity" >
		</activity>
    </application>

</manifest>
